{
  "name": "svelte-app",
  "version": "1.0.0",
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/preset-env": "^7.16.11",
    "@ota-meshi/eslint-plugin-svelte": "0.26.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/svelte": "^3.0.3",
    "@testing-library/user-event": "^13.5.0",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "babel-jest": "^27.5.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.5.1",
    "nollup": "^0.13.13",
    "peerjs": "^1.3.2",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-hot": "^0.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte-hot": "^0.13.0",
    "rollup-plugin-terser": "^7.0.0",
    "source-map": "^0.8.0-beta.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.6.0",
    "svelte-jester": "^2.3.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "js-confetti": "^0.10.2",
    "sirv-cli": "^1.0.0",
    "svelte-boring-avatars": "^1.2.2",
    "svelte-local-storage-store": "^0.2.6",
    "sweetalert2": "^11.4.8"
  },
  "resolutions": {
    "source-map": "^0.8.0-beta.0"
  },
  "scripts": {
    "build": "rollup -c",
    "dev:rollup": "rollup -c -w",
    "dockerall": "yarn dockerbuild && yarn dockerstop && yarn dockerrun",
    "dockerstop": "docker stop crypto && docker rm crypto",
    "dockerbuild": "docker build --tag cryptoduel .",
    "dockerrun": "docker run -d -p 5000:5000 --name crypto cryptoduel",
    "dev:livereload": "rollup -c -w --environment LIVERELOAD",
    "dev:lan": "npm run dev:livereload & npm run start -- --dev --host 0.0.0.0",
    "dev:nollup": "nollup -c --hot --port 5000 --content-base ./public",
    "dev": "npm run dev:nollup",
    "test": "jest src",
    "test:watch": "jest src --watch",
    "type:check": "svelte-check --tsconfig tsconfig.json",
    "type:check:watch": "svelte-check --watch --tsconfig tsconfig.json",
    "start": "sirv public --host",
    "format": "eslint --cache --fix src"
  }
}
